document.images['imageWU'].src = imgWU.src;
////////////////// header loop ///////////////////////////////
headerImages = new Array();
headerImages[0] = "images/header/header1.jpg";
headerImages[1] = "images/header/header2.jpg";
headerImages[2] = "images/header/header3.jpg";
headerImages[3] = "images/header/header4.jpg";
numberOfHeaders = 4;
currentHeaderIdx = 0;
ie5  = (document.all && document.getElementById);
ns6 = (!document.all && document.getElementById);
var opac = 0;
try
{
	if (!disableLoopHeader)
		changeBGHeader();
}
catch (e)
{
	// disableLoopHeader undefined
	changeBGHeader();
}

function changeBGHeader()
{
	opac = 0;
	var verticalPosition = Math.round(100*Math.random());
	var header = document.getElementById('header');
	var inURL = headerImages[currentHeaderIdx];
	header.style.backgroundImage = "url(" + inURL + ")";
	header.style.backgroundPosition = "0% " + verticalPosition + "%";
	//var Timer = setInterval("FadeIn()", 1);
	currentHeaderIdx++;
	if (currentHeaderIdx == numberOfHeaders)
		currentHeaderIdx = 0;
	fadeIn();
	setTimeout("changeBGHeader()", 8000);
}
function fadeOut() {
    if(opac > 0){
        opac-=20;
        if(ie5) document.getElementById('header').style.filter = "Alpha(Opacity=" + opac + ")";
        if(ns6) document.getElementById('header').style.MozOpacity = opac/100;
        setTimeout('fadeOut()', 1);
    }
}
function fadeIn() {
    if(opac < 100){
        opac+=20;
        if(ie5) document.getElementById('header').style.filter = "Alpha(Opacity=" + opac + ")";
        if(ns6) document.getElementById('header').style.MozOpacity = opac/100;
        setTimeout('fadeIn()', 1);
    }
}
//////////////////////////////////////////////////////////////
function toggleBrokenData (ahref)
{
	var tdOfTitle = ahref.parentNode;
	var trOfTitle = tdOfTitle.parentNode;
	var nextTR = trOfTitle.nextSibling;
	while(nextTR)
	{
		if (nextTR.style.display == "none")
			nextTR.style.display = "";
		else
			nextTR.style.display = "none";
		nextTR = nextTR.nextSibling;
	}
}
function changeLang (lang)
{
	var loc = "http://jweather.dyndns.org/weather/Station/station.php";
	loc = loc.replace(/lang=\d/g, "lang=" + lang);
	if (loc.indexOf('?') < 0)
	{
		loc = loc + "?";
	}
	if (loc.indexOf('lang') < 0)
	{
		loc = loc + "&lang=" + lang;
	}
	top.location.href=loc;
}

