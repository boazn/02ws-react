	function getMessageService(filter, start, update)
	{	
		
		var name = document.getElementById('name').innerHTML;
		var body = document.getElementById('body').value;
		var mood_img = document.getElementById('mood_img');
		var mood_elm = "";
		if ((mood_img != null)&&(mood_img != 'undfined')) 
		{
			mood_elm = "<img align=\"absmiddle\" src=\"" + mood_img.src + "\" width=\"22\" alt=\"" + mood_img.alt + "\" title=\"" + mood_img.alt + "\" />";
		}
		
		var searchname = document.getElementById('searchname');
		if (searchname != null)
			searchname = searchname.value;
			else
				 searchname = '';
		
		if ((filter == '') && 
			((searchname == '') || (searchname == null)) && 
			((name == '' ||  body == '' || body == 'הודעה' || name == 'שם')))
		{
			return false;
		}
		
		var idx = "";
                var msgDetails = document.getElementById('msgDetails');
		var idxs = msgDetails.getElementsByTagName("input"); 
		for (var i = 0; i < idxs.length; i++) { 
			if (idxs[i].type=="checkbox")  
				if (idxs[i].checked) { 
					idx = idxs[i].value; 
				}
		}
		var limit = "4";
		if ((filter != "") && (filter != "undefined"))
			limit = filter;
		if (update == 0)
		{
			body = '';
			name = '';
		}
		
		restoreTopDiv();
		
		var postData = "lang=" + "1" + "&limit=" + limit + "&startLine=" + start + "&name=" + escape(encodeURI(name)) + "&searchname=" + escape(encodeURI(searchname)) + "&body=" + escape(encodeURI(body)) + "&mood=" + escape(encodeURI(mood_elm)) + "&idx=" + idx + "&update=";
		fillMessage('<img src="images/loading.gif" alt="loading" />');
		$('input[name="SendButton"]').attr("disabled", "disabled");
		var ajax = new Ajax();
		ajax.method = 'POST';
		ajax.setMimeType('text/html');
		ajax.postData = postData;
		ajax.setHandlerBoth(fillMessage);
		ajax.url = 'chat_service.php';
		ajax.doReq();
		
	}

       
        function startup(page)
        {
        	if (document.getElementById('chatWrapper'))
		{
                        $.ajax({
			  type: "GET",
			  url: "checkauth.php?action=getuser&lang=1"
			}).done(function( jsonstr ) {
			  try{
	                     
	                     var jsonT = JSON.parse( jsonstr  );
	                     if (!jsonT.user.loggedin)
	                         {
	                            toggle('notloggedin');
	                            if (jsonT.user.locked)
	                                alert('חשבון חסום');
	                         }
	                       else {
	                         toggle('loggedin'); 
