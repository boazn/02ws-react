		   {
			   index = "indexmedium";
			    title = "בינוני";
		   }
		   else if (airq_value >  -200)
		   {
			   index = "indexhigh";
			    title = "גבוה";
		   }
		   else 
		   {
			   index = "indexextreme";
			    title = "קיצוני";
		   }
		  
		return "<span class='" + index + "'>" + title + "</span>" ;
	}

