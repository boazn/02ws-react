#AddHandler fcgid-script .php
AddHandler application/x-httpd-php5s .php
RewriteEngine On

RewriteCond %{HTTP_HOST} ^02ws.com$

RewriteRule ^$ http://www.02ws.com/station.php [L,R=301]

#If a URL which maps to a directory is requested, and there is no DirectoryIndex (e.g., index.html) in that directory, then mod_autoindex will return a formatted listing of the directory.
#Options -indexes

Redirect /images/index.php /images/webCamera.jpg

FileETag none

# Turn on Expires and set default to 0

#ExpiresDefault A0
ExpiresActive On
 
# Set up caching on media files for 1 year (forever?)
<FilesMatch "\.(flv|ico|pdf|avi|mov|ppt|doc|mp3|wmv|wav|swf)$">
ExpiresDefault A29030400
Header append Cache-Control "public"
</FilesMatch>
 
# Set up caching on media files for 1 week
<FilesMatch "\.(png)$">
ExpiresDefault "access plus 1 years"
Header append Cache-Control "public"
</FilesMatch>

# Set up caching on script files for 1 week
#<FilesMatch "\.(js)$">
ExpiresDefault A604800
Header append Cache-Control "public"
#</FilesMatch>

<FilesMatch "buttongradient.php$">
ExpiresDefault "access plus 10 years"
Header append Cache-Control "public"
</FilesMatch>

<IfModule mod_deflate.c>
AddOutputFilterByType DEFLATE text/html text/plain text/xml text/css text/javascript application/javascript application/x-javascript
BrowserMatch ^Mozilla/4 gzip-only-text/html
BrowserMatch ^Mozilla/4\.0[678] no-gzip
BrowserMatch \bMSIE !no-gzip !gzip-only-text/html
<FilesMatch “\.(js|css|ttf)$”>
SetOutputFilter DEFLATE
</FilesMatch>
</IfModule>
<IfModule mod_expires.c>
ExpiresActive On
#ExpiresByType image/gif "access plus 10 years"
ExpiresByType image/jpeg "access plus 300 seconds"
#ExpiresByType image/png "access plus 1 years"
ExpiresByType text/css "access plus 1 weeks"
ExpiresByType text/javascript "access plus 1 weeks"
#ExpiresByType application/x-unknown-content-type "access plus 10 years"
#ExpiresByType application/x-javascript "access plus 10 years"
#ExpiresByType text/html "access plus 1 seconds"
</IfModule>
<FilesMatch "\.(jpg|jpeg|gif)$">
        Header unset Etag
        Header set Cache-control "public, max-age=600"
 </FilesMatch>

# protect phpinfo
<Files phpinfo.php>
Order Deny,Allow
Deny from all
</Files>